{
  "packages": ["frontend/**/*", "backend/**/*", "types/**/*"],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "independent",
  "command": {
    "version": {
      "allowBranch": ["dev", "master"],
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): publish"
    }
  }
}
